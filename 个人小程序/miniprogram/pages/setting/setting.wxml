<!--miniprogram/pages/setting/setting.wxml-->
<view class='items' style='height:150rpx;'>
  <text>用户头像</text>
  <image class='avatarUrl' src='{{userInfo.avatarUrl}}'></image>
</view>
<view class='items'>
