  <text>{{userInfo.nickName}}</text>
</view>
<navigator hover-class='none' url='../setPassword/setPassword' class='items'>
  <text>设置密码</text>
  <image class='icon' src='cloud://hua-7e98f2.6875-hua-7e98f2/setting_right.png'></image>
</navigator>